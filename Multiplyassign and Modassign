public class MultiplyAssignExample {
    public static void main(String[] args) {
        int x = 6;
        x *= 2;  // x = x * 2 → 12
        System.out.println("x = " + x);
    }
}
public class ModAssignExample {
    public static void main(String[] args) {
        int x = 17;
        x %= 5;  // x = x % 5 → 2
        System.out.println("x = " + x);
    }
}
