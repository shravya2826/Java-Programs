public class GreaterThanOrEqualExample {
    public static void main(String[] args) {
        int x = 20, y = 20;
        System.out.println("x >= y: " + (x >= y));  // true
    }
}
public class LessThanOrEqualExample {
    public static void main(String[] args) {
        int x = 8, y = 10;
        System.out.println("x <= y: " + (x <= y));  // true
    }
    }
