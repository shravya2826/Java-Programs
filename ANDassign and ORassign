public class AndAssignExample {
    public static void main(String[] args) {
        int x = 12; // 1100
        x &= 10;    // 1100 & 1010 = 1000 (8)
        System.out.println("x = " + x);
    }
}
public class OrAssignExample {
    public static void main(String[] args) {
        int x = 12; // 1100
        x |= 10;    // 1100 | 1010 = 1110 (14)
        System.out.println("x = " + x);
    }
}
